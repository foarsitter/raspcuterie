[Unit]
Description=Raspcuterie dashboard
After=network.target

[Service]
User=jelmert
Environment=FLASK_APP=raspcuterie.wsgi:app
Environment=FLASK_ENV=development
WorkingDirectory=/home/jelmert/PycharmProjects/raspcuterie
ExecStart=/home/jelmert/PycharmProjects/raspcuterie/venv/bin/flask run --host 0.0.0.0 --port 1925
Restart=always

[Install]
WantedBy=multi-user.target